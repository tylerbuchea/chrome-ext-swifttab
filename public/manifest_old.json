{
  "manifest_version": 2,
  "name": "SwiftTab",
  "description":
    "Personal new tab page featuring links from your Bookmarks Bar",
  "version": "0.1.0",
  "homepage_url": "https://github.com/tylerbuchea/chrome-ext-swifttab",
  "author": "Tyler Buchea",
  "permissions": ["bookmarks", "chrome://favicon/"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
